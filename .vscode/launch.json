{
    "configurations": [
        {
            "name": "C/C++: g++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [
                "-w",
                "-I../include",
                "-O2",
                "-std=c++11",
                "-pedantic",
                "-W",
                "-Wall",
                "-Wshadow",
                "-fPIC",
                "-pthread",
                "-L../lib",
                "-Wl,-rpath,../lib",
                "-lpythia8",
                "-ldl",
                "-I/data/theorie/pherbsch/ML4EFT/packages/fastjet-install/include",
                "-L/data/theorie/pherbsch/ML4EFT/packages/fastjet-install/lib",
                "-Wl,-rpath,/data/theorie/pherbsch/ML4EFT/packages/fastjet-install/lib",
                "-lfastjet",
                "-lz"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPrintSize": true,
            "preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/data/theorie/pherbsch/miniconda3/envs/madgraph/bin/gdb"
        }
    ],
    "version": "2.0.0"
}